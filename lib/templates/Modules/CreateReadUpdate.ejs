import { Module } from "../../../core/types.core";
import <%= fileName %>Controller from "./<%= fileName.toLowerCase() %>.controller";

export default class <%= fileName %>Module extends Module {
  private controller = new <%= fileName %>Controller();
  constructor() {
    super();
    this.baseURL = "/<%= fileName.toLowerCase() %>";
    this.subRoutes = [
      {
        method: "get",
        path: "/",
        control: this.controller.index,
      },
      {
        method: "get",
        path: "/:id",
        control: this.controller.read,
      },
      {
        method: "post",
        path: "/",
        control: this.controller.create,
      },
      {
        method: "patch",
        path: "/:id",
        control: this.controller.update,
      },
      {
        method: "put",
        path: "/:id",
        control: this.controller.update,
      },
    ];
  }
}

