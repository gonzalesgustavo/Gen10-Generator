import { Methods, Module, RouteObject } from '../../../Core/types.core';
import <%= fileName %>Controller from './<%= fileName.toLowerCase() %>.controller';

export default class <%= fileName %>Module implements Module {
  baseURL: string;
  subRoutes: RouteObject[];
  constructor(){
    this.baseURL = '<%= fileName.toLowerCase() %>';
    this.subRoutes = [
      {
        path: '',
        method: Methods.GET,
        control: <%= fileName %>Controller.index,
      },
      {
        path: '/:id',
        method: Methods.GET,
        control: <%= fileName %>Controller.read,
      },
      {
        path: '/',
        method: Methods.POST,
        control: <%= fileName %>Controller.create,
      },
      {
        path: '/:id',
        method: Methods.PUT,
        control: <%= fileName %>Controller.update,
      },
      {
        path: '/:id',
        method: Methods.PATCH,
        control: <%= fileName %>Controller.update,
      },
      {
        path: '/:id',
        method: Methods.DELETE,
        control: <%= fileName %>Controller.remove,
      },
    ]
  }
}

